    "@powerhousedao/reactor-api": "^4.0.4",
    "@powerhousedao/reactor-browser": "4.0.4",
    "@powerhousedao/reactor-local": "4.0.4",
    "@powerhousedao/scalars": "2.0.1",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/addon-links": "^9.1.1",
    "@storybook/builder-vite": "^9.1.1",
    "@storybook/components": "^8.6.14",
    "@storybook/manager-api": "^8.6.14",
    "@storybook/react": "^9.1.1",
    "@storybook/react-vite": "^9.1.1",
    "@tailwindcss/cli": "^4.0.15",
    "@testing-library/react": "^16.0.1",
    "@types/node": "^22.13.11",
    "@types/react": "^18.3.19",
    "@vitejs/plugin-react": "^4.3.4",
    "document-drive": "^4.0.4",
    "eslint": "^9.22.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.0.0",
    "package-manager-detector": "^1.1.0",
    "pm2": "^5.4.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "storybook-mock-date-decorator": "^3.0.0",
    "tailwindcss": "^4.0.15",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.27.0",
    "vite": "^6.2.3",
    "vitest": "^3.0.9"
  },
  "peerDependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "description": ""
