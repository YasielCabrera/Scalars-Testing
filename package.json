    "lucide-react": "^0.484.0",
    "react-json-view": "^1.21.3",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@powerhousedao/analytics-engine-core": "^0.5.0",
    "@powerhousedao/codegen": "4.0.4",
